                    }
                },
                {
                    "name": "url",
                    "selector": {
                        "css": "[class*='wcl-nameWrapper']",
                        "attribute": "href"
                    },
                    "options": {
                        "required": false,
                        "type": "str"
                    }
                },
                {
                    "name": "description",
                    "selector": {
                        "css": "[class*='wcl-description']",
                        "attribute": "text"
